<div class="uiminiform">
<div id="formbox">
<h1>Log In</h1>
<p id="alert"><%= alert %></p>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %> <br><br>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %><br><br>
    <%= password_field_tag :password %>
  </div>
  <button class="actions">Log In</button>
  <% if current_user then redirect_to dogs_path %>
<% end %>
<% end %>
</div>
</div>